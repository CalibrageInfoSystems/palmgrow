<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <LinearLayout
        android:id="@+id/parent_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/common_margin"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_top_layout"
            android:orientation="horizontal">

            <TextView
                style="@style/header_h1_text_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:text="@string/account_holder_name" />

            <EditText
                android:id="@+id/account_holder_name"
                style="@style/editTxt_normal"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:digits="@string/input_type_name"
                android:maxLength="30"
                android:textAllCaps="true" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_top_layout"
            android:orientation="horizontal">

            <TextView
                style="@style/header_h1_text_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:text="@string/account_number" />

            <EditText
                android:id="@+id/account_number"
                style="@style/editTxt_normal"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:inputType="number"
                android:maxLength="30" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_top_layout"
            android:orientation="horizontal">

            <TextView
                style="@style/header_h1_text_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:text="@string/ifsc_code" />

            <AutoCompleteTextView
                android:id="@+id/ifsc_code"
                style="@style/editTxt_normal"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:maxLength="30"  />
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_top_layout"

            android:orientation="horizontal">

            <TextView
                style="@style/header_h1_text_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:text="Bank Name" />

            <EditText
                android:id="@+id/bankCodeSpin"
                style="@style/editTxt_normal"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:focusable="false"
                android:layout_gravity="center"
                android:cursorVisible="false"
                android:clickable="false"
                android:text=""
                android:textColor="@color/black"
                android:enabled="false"
                android:layout_weight="1"
                />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_top_layout"
            android:orientation="horizontal">

            <TextView
                style="@style/header_h1_text_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:text="Branch Name" />
            <EditText
                android:id="@+id/branchNameSpin"
                style="@style/editTxt_normal"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:focusable="false"
                android:textColor="@color/black"
                android:layout_gravity="center"
                android:text=""
                android:cursorVisible="false"
                android:clickable="false"
                android:enabled="false"
                android:layout_weight="1"
                />



        </LinearLayout>


        <LinearLayout
            android:id="@+id/farmerphotoLL"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginTop="@dimen/margin_top_layout"
            android:orientation="vertical"
            android:weightSum="3">

            <TextView
                android:id="@+id/farmerphotoTitle"
                style="@style/header_h1_text_title_with_background"
                android:textColor="@color/black"
                android:text="Capture Bank PassBook*"
                android:textSize="20dp"/>



            <RelativeLayout
                android:layout_width="200dp"
                android:layout_height="200dp"
                android:layout_gravity="center"
                android:layout_marginTop="20dp"
                android:layout_weight="2"
                android:background="@mipmap/ic_rsz_framepng">


                <ImageView
                    android:id="@+id/bankicon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/farmer_image"
                    android:layout_alignParentTop="true"
                    android:src="@mipmap/ico_btn_photo" />

                <ImageView
                    android:id="@+id/passbookImage"
                    android:layout_width="250dp"
                    android:layout_height="250dp"
                    android:layout_gravity="center"
                    android:layout_margin="8dp"
                    android:layout_weight="1"
                    android:elevation="4dp"
                    />
            </RelativeLayout>


        </LinearLayout>



        <LinearLayout
            android:id="@+id/otherBankLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin_top_layout"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                style="@style/header_h1_text_title"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1"
                android:text="@string/nameofthebank" />

            <EditText
                android:id="@+id/bankNameText"
                style="@style/editTxt_normal"
                android:layout_width="0dp"
                android:layout_height="50dp"
                android:layout_weight="1"
                android:digits="@string/digits"
                android:maxLines="30" />
        </LinearLayout>


